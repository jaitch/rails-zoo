<p>Please enter the information for the new animal: </p>

<%= form_with model: @animal do |f| %>

<%= f.label "Animal's Name:" %>
<%= f.text_field :name %>
<br>
<%= f.label "Species:" %>
<%= f.text_field :species %>
<br>
<%= f.label "Age:" %>
<%= f.text_field :age %>
<br>
<%= f.submit "update" %>

<% end %>
